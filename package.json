{
  "name": "vscode-typo3-fluid",
  "displayName": "Typo3 Fluid Syntax Highlighting",
  "description": "Syntax highlighting for TYPO3 Fluid",
  "version": "0.1.1",
  "publisher": "leon-wbr",
  "icon": "icons/typo3.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leon-wbr/vscode-typo3-fluid/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leon-wbr/vscode-typo3-fluid"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "typo3",
    "fluid"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fluid",
        "aliases": [
          "Fluid",
          "fluid"
        ],
        "extensions": [
          ".html"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "icons/typo3-transparent.png",
          "dark": "icons/typo3-transparent.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "fluid",
        "scopeName": "source.fluid",
        "path": "./syntaxes/fluid.tmLanguage.json"
      }
    ]
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "devDependencies": {
    "vscode-test": "^1.6.1"
  }
}
