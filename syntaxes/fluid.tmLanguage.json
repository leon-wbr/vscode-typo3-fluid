{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Fluid",
  "scopeName": "source.fluid",
  "fileTypes": ["html"],
  "patterns": [
    {
      "include": "text.html.derivative"
    },
    {
      "include": "#fluid-tags"
    }
  ],
  "repository": {
    "fluid-tags": {
      "patterns": [
        {
          "name": "entity.name.tag.fluid",
          "match": "<f:[a-zA-Z]+"
        }
      ]
    }
  }
}
